<?xml version="1.0" encoding=""?>
<interface>
    <template class="UchatAvatarBox" parent="GtkWidget">
        <child>
            <object class="GtkBox">
                <child>
                    <object class="GtkDrawingArea" id="area">
                        <property name="content-width">100</property>
                        <property name="content-height">100</property>
                        <style>
                            <class name="avatar"/>
                        </style>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>